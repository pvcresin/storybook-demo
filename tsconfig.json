{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    // "lib": [],
    // "allowJs": true,
    // "checkJs": true,
    "jsx": "react",
    // "declaration": true,
    // "declarationMap": true,
    "sourceMap": true,
    // "outFile": "./",
